#rate_limit="700"
#retry_limit="3"
instance_plan     = "vc2-1c-1gb"
instance_region   = "mia"
instance_os_id    = "1743"
instance_label    = "example-vps"
instance_hostname = "example-vps"
#instance_enable_ipv6=false
#instance_ddos_protection=true
vultr_ssh_key_name = "devops_sshkey"
vultr_ssh_key      = "~/.ssh/id_rsa.pub"
vultr_firewall_groupname="example_fw_vps"
fw_rule_protocol="tcp"
fw_rule_ip_type="v4"
fw_rule_subnet="0.0.0.0"	
fw_rule_subnet_size="0"
fw_rule_port="22"
fw_rule_notes="public ssh access"
